<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ADB Viewer resource DLL" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="../bin/Debug/ADBViewer" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-std=c++17" />
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-D_BUILD_DLL" />
					<Add option="-D_BUILD_FRAME_NO_TITLE" />
					<Add directory="C:/__BuildSource/__LIB__/SDL2/include" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="../bin/Release/ADBViewer" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-std=c++17" />
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-D_BUILD_DLL" />
					<Add option="-D_BUILD_FRAME_NO_TITLE" />
					<Add directory="C:/__BuildSource/__LIB__/SDL2/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<ExtraCommands>
					<Add after="upx -9 ../bin/Release/ADBViewer.dll" />
				</ExtraCommands>
			</Target>
		</Build>
		<Linker>
			<Add option="-static-libstdc++" />
			<Add option="-static-libgcc" />
			<Add option="-lmingw32" />
			<Add option="-lSDL2.dll" />
			<Add option="-luser32" />
			<Add directory="C:/__BuildSource/__LIB__/SDL2/lib" />
		</Linker>
		<Unit filename="src/ADBViewerDLL.cpp" />
		<Unit filename="src/ADBViewerDLL.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="src/ResManager.cpp" />
		<Unit filename="src/ResManager.h" />
		<Unit filename="src/ResManagerPopUp.cpp">
			<Option compiler="gcc" use="1" buildCommand="$compiler -std=c++17 -finput-charset=cp1251 $options $includes -c $file -o $object" />
		</Unit>
		<Unit filename="src/Resources/ResHelp.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
