<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ADB Driver Pack DLL" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="../bin/Debug/ADBDriverPack" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-g" />
					<Add directory="../ADBDriver/src" />
				</Compiler>
				<Linker>
					<Add option="-luser32" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="../bin/Release/ADBDriverPack" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add directory="../ADBDriver/src" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-luser32" />
				</Linker>
				<ExtraCommands>
					<Add after="upx -4 ../bin/Release/ADBDriverPack.dll" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++17" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-Wno-unknown-pragmas" />
			<Add option="-D_BUILD_DLL=1" />
		</Compiler>
		<Unit filename="src/ADBDriverPackDLL.cpp" />
		<Unit filename="src/ADBDriverPackDLL.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="src/PackManager.cpp" />
		<Unit filename="src/PackManager.h" />
		<Unit filename="src/extern/incbin.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
